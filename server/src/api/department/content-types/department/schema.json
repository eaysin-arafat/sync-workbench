{
  "kind": "collectionType",
  "collectionName": "departments",
  "info": {
    "singularName": "department",
    "pluralName": "departments",
    "displayName": "Department",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "department_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "location": {
      "type": "string"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "department_id"
    },
    "description": {
      "type": "string"
    },
    "manager": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::employee.employee",
      "inversedBy": "employee_of_departments"
    },
    "employees": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::employee.employee",
      "mappedBy": "manager_of_department"
    }
  }
}
