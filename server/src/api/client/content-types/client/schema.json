{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "industry": {
      "type": "string",
      "required": true
    },
    "contact_email": {
      "type": "email"
    },
    "contact_phone": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "client"
    },
    "time_sheets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::time-sheet.time-sheet",
      "mappedBy": "client"
    }
  }
}
