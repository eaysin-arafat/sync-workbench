{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "phone_number": {
      "required": true,
      "type": "integer"
    },
    "date_of_birth": {
      "type": "date"
    },
    "address": {
      "type": "text",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "zip_code": {
      "type": "integer",
      "required": true
    },
    "date_of_hire": {
      "type": "date",
      "required": true
    },
    "job_title": {
      "type": "string",
      "required": true
    },
    "department_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::department.department"
    },
    "manager_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "position_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::application-role.application-role"
    },
    "salary": {
      "type": "biginteger"
    },
    "employment_status": {
      "type": "string",
      "required": true
    },
    "profile_picture": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "employee_status": {
      "type": "string",
      "required": true
    },
    "attendances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attendance.attendance",
      "mappedBy": "employee_id"
    },
    "leaves": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::leave.leave",
      "mappedBy": "employee_id"
    },
    "performance_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::performance-review.performance-review",
      "mappedBy": "employee_id"
    },
    "performance_review": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::performance-review.performance-review",
      "mappedBy": "reviewer_id"
    },
    "document": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::document.document",
      "mappedBy": "employee_id"
    },
    "employee_skill": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::employee-skill.employee-skill",
      "mappedBy": "employee_id"
    },
    "employee_permissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::employee-permission.employee-permission",
      "mappedBy": "employee_id"
    },
    "employee_certifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::employee-certification.employee-certification",
      "mappedBy": "employee_id"
    }
  }
}
