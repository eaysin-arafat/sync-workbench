{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "department": {
      "type": "string",
      "required": true
    },
    "designation": {
      "type": "string",
      "required": true
    },
    "dateOfJoining": {
      "type": "date",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "profilePicture": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": false
    },
    "user_off_days": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-off-day.user-off-day",
      "mappedBy": "users_permissions_user"
    },
    "user_holidays": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-holiday.user-holiday",
      "mappedBy": "users_permissions_user"
    },
    "requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::request.request",
      "mappedBy": "users_permissions_user"
    },
    "time_sheets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::time-sheet.time-sheet",
      "mappedBy": "users_permissions_user"
    },
    "missed_time_sheets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::missed-time-sheet.missed-time-sheet",
      "mappedBy": "users_permissions_user"
    }
  }
}
